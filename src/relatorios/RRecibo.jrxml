	<queryString>
		<![CDATA[select empresa.`nome` AS nome_empresa, empresa.`localizacao` AS localizacao_empresa, empresa.`email`
 AS localizacao_email, empresa.`telefone` 
 AS telefone_empresa, empresa.`nif`
  AS nif_empresa, empresa.`telemovel` 
  AS telemovel_empresa, empresa.`logo` AS logo, utilizador.`ut_nome` 
  AS nomeOperador, pagamento.`codPagamento` AS codFatura, pagamento.`tipo` As tipoPagamento, pessoa.`morada` 
  AS morada_pessoa, pessoa.`nome` AS nome_pessoa, pessoa.`telemovel` AS telemovel_pessoa, matriculamodalidade.`valorModalidade` 
  AS valorModal, modalidade.`nome` AS nomeModal
 from pagamento inner join pessoa on pagamento.idCliente = pessoa.idPessoa 
inner join matricula on pagamento.idControlPagamento = matricula.idMatricula inner join empresa 
inner join utilizador on pagamento.idFuncionario = utilizador.ut_IdUtilizador 
inner join matriculamodalidade on matricula.idMatricula = matriculamodalidade.idMatricula
inner join modalidade on matriculamodalidade.idModalidade = modalidade.idModalidade
WHERE pagamento.codPagamento =$P{idPag}]]>
	</queryString>
