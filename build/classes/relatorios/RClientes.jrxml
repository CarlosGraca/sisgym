	<queryString>
		<![CDATA[SELECT
     empresa.`logo` AS empresa_logo,
     pessoa.`nome` AS Nome,
     pessoa.`morada` AS Morada,
     pessoa.`telemovel` AS Telemovel,
     pessoa.`profissao` AS Profissao,
     pessoa.`sexo` As Sexo,
     pessoa.`dtNasc` As Data,
     pessoa.`status` AS Status
